<table class="table">
  <thead>
  <tr>
    <th colspan="1"></th>
    <th>average</th>
  </tr>
  </thead>
  <tbody>
  <% list_items.each do |item| %>
      <tr>
        <td><%= link_to item.name, item %></td>
        <td><%= round item.average_rating %></td>
      </tr>
  <% end %>
  </tbody>
</table>